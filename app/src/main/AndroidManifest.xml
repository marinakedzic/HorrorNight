<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.filip.horrornight">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".EditProfile"></activity>
        <activity android:name=".ShowPlayer" />
        <activity android:name=".PlayerScreen" />
        <activity android:name=".story.Forest" />
        <activity android:name=".story.Run" />
        <activity android:name=".story.GoInside" />
        <activity android:name=".End" />
        <activity android:name=".story.GoOutside" />
        <activity android:name=".story.StayHome" />
        <activity android:name=".Start" />
        <activity android:name=".PlayerInfo" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>